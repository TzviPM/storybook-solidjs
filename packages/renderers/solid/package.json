{
  "name": "@tzvipm.dev/storybook-solidjs",
  "version": "0.1.0",
  "description": "Storybook SolidJS renderer",
  "keywords": [
    "storybook"
  ],
  "homepage": "https://github.com/storybookjs/storybook/tree/main/renderers/solid",
  "bugs": {
    "url": "https://github.com/storybookjs/solidjs/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/storybookjs/solidjs.git",
    "directory": "renderers/solid"
  },
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/storybook"
  },
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./preset": "./preset.js",
    "./dist/entry-preview.mjs": "./dist/entry-preview.mjs",
    "./dist/entry-preview-docs.mjs": "./dist/entry-preview-docs.mjs",
    "./package.json": "./package.json"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "template/**/*",
    "README.md",
    "*.js",
    "*.d.ts"
  ],
  "scripts": {
    "prepack": "yarn build --optimized --reset",
    "build": "vite build && tsc src/preset.ts --outDir dist --declaration && tsc --emitDeclarationOnly --declaration --outDir dist",
    "check": "tsc --noEmit"
  },
  "dependencies": {
    "@babel/standalone": "^7.26.2",
    "@storybook/global": "^5.0.0",
    "async-mutex": "^0.5.0"
  },
  "devDependencies": {
    "storybook": "latest",
    "vite": "^6.0.0",
    "vite-plugin-solid": "^2.10.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "solid-js": "~1.9.0",
    "storybook": "*"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "bundler": {
    "entries": [
      "./src/index.ts",
      "./src/preset.ts",
      "./src/entry-preview.tsx",
      "./src/entry-preview-docs.tsx"
    ],
    "platform": "browser"
  }
}
